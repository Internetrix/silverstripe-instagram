{
  "name": "internetrix/silverstripe-instagram",
  "description": "Implements extensions that allows caching and displaying of Instagram feed data",
  "type": "silverstripe-vendormodule",
  "homepage": "https://github.com/Internetrix/silverstripe-instagram/",
  "keywords": ["silverstripe", "module", "instagram", "graphql"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Drew Carrette",
      "email": "drew.carrette@internetrix.com.au"
    }
  ],
  "support": {
    "issues": "https://github.com/Internetrix/silverstripe-instagram/issues"
  },
  "require": {
    "silverstripe/vendor-plugin": "^1.0",
    "silverstripe/framework": "~4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "extra": {
    "installer-name": "template"
  },
  "autoload": {
    "psr-4": {
      "Internetrix\\Instagram\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
